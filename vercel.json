{
  "version": 2,
  "functions": {
    "api/pdf.js": {
      "memory": 3008,
      "maxDuration": 60
    },
    "api/bot.js": {
      "memory": 1024,
      "maxDuration": 60
    },
    "api/setup-webhook.js": {
      "memory": 512,
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/api/pdf",
      "dest": "/api/pdf.js"
    },
    {
      "src": "/pdf",
      "dest": "/api/pdf.js"
    },
    {
      "src": "/api/bot",
      "dest": "/api/bot.js"
    },
    {
      "src": "/bot",
      "dest": "/api/bot.js"
    }
  ],
  "functions": {
    "api/**/*.js": {
      "memory": 3008,
      "maxDuration": 60
    }
  }
}
